{
  "name": "sf-chain",
  "description": "A safe chain utility library for handling asynchronous operations in a type-safe manner.",
  "author": "cging",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "safe-chain",
    "async",
    "utility",
    "sf-chain",
    "functional-programming",
    "fp",
    "monad",
    "error-handling",
    "composable",
    "result"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "vitest",
    "lint-staged": "lint-staged",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "format": "prettier --write ./**/*.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.5",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.2.5",
    "tsup": "^8.3.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cgoinglove/sf-chain.git"
  }
}
